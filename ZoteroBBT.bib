@article{zhang_charge_2006,
  title = {Charge and Discharge Characteristics of a Commercial {{LiCoO2}}-Based 18650 {{Li}}-Ion Battery},
  author = {Zhang, S. S. and Xu, K. and Jow, T. R.},
  year = {2006},
  month = oct,
  volume = {160},
  pages = {1403--1409},
  issn = {0378-7753},
  doi = {10.1016/j.jpowsour.2006.03.037},
  abstract = {We studied the charge and discharge characteristics of commercial LiCoO2-based 18650 cells by using various electrochemical methods, including discharging at constant power, ac impedance spectroscopy, and dc-voltage pulse. At 20\textdegree{}C, these cells deliver 8.7\textendash{}6.8Wh of energy when discharged at a power range of 1\textendash{}12W between 2.5 and 4.2V. Ragone plots show that the effect of discharge power on the energy is significantly increased with decreasing of the temperature. For example, energy of the cell is entirely lost when the temperature downs to -10\textdegree{}C and the discharge rate still remains at 10W. Impedance analyses indicate that the total cell resistance (Rcell) is mainly contributed by the bulk resistance (Rb, including electric contact resistance and electrolytic ionic conductivity), solid electrolyte interface resistance (Rsei), and charge-transfer resistance (Rct). Individual contribution of these three resistances to the cell resistance is greatly varied with the temperature. Near room temperature, the Rb occupies up to half of the cell resistance, which means that the rate performance of the cell could be improved by modifying cell design such as employing electrolyte with higher ionic conductivity and enhancing electric contact of the active material particles. At low temperature, the Rct, which is believed to reflect cell reaction kinetics, dominates the cell resistance. In addition, galvanosatic cycling tests indicate that the charge and discharge processes have nearly same kinetics. The performance discrepancy observed during charging and discharging, especially at low temperatures, can be attributed to these two factors of: (1) substantially higher Rct at the discharged state than at the charged state; (2) asymmetric voltage limits pre-determined for the charge and discharge processes.},
  file = {/Users/tn438/Zotero/storage/6ZHK3JHN/S0378775306004447.html},
  journal = {Journal of Power Sources},
  keywords = {18650 Li-ion battery,Charge,Constant power,Discharge,Impedance,Low temperature},
  language = {en},
  number = {2},
  series = {Special Issue Including Selected Papers Presented at the {{International Workshop}} on {{Molten Carbonate Fuel Cells}} and {{Related Science}} and {{Technology}} 2005 Together with Regular Papers}
}

