}

@article{westman_diglyme_2018,
  title = {Diglyme {{Based Electrolytes}} for {{Sodium}}-{{Ion Batteries}}},
  author = {Westman, K. and Dugas, R. and Jankowski, P. and Wieczorek, W. and Gachot, G. and Morcrette, M. and Irisarri, E. and Ponrouch, A. and Palac{\'i}n, M. R. and Tarascon, J.-M. and Johansson, P.},
  year = {2018},
  month = jun,
  volume = {1},
  pages = {2671--2680},
  doi = {10.1021/acsaem.8b00360},
  abstract = {Sodium-ion batteries (SIBs) are currently being considered for large-scale energy storage. Optimization of SIB electrolytes is, however, still largely lacking. Here we exhaustively evaluate NaPF6 in diglyme as an electrolyte of choice, via both physicochemical properties and extensive electrochemical tests including half as well as full cells. Fundamentally, the ionic conductivity is found to be quite comparable to carbonate based electrolytes and to obey the fractional Walden rule with viscosity. We find Na metal to work well as a reference electrode and the electrochemical stability, evaluated potentiostatically for various electrodes and corroborated by DFT calculations, to be satisfactory in the entire voltage range 0\textendash{}4.4 V. Galvanostatic cycling at C/10 of half and full cells using Na3V2(PO4)3 (NVP) or Na3V2(PO4)2F3 (NVPF) as cathodes and hard carbon (HC) as anodes indicates rapid capacity fading in cells with HC anodes, possibly originating in a lack of a stable SEI or by trapping of sodium. Aiming to understand this capacity fade further, we conducted a GC/MS analysis to determine electrolyte reduction products and to propose reduction pathways, concluding that oligomer and/or alkoxide formation is possible. Overall, the promising results should warrant further investigations of diglyme based electrolytes for modern SIB development, albeit avoiding HC anodes.},
  file = {/Users/tn438/Zotero/storage/X4YZA6Y5/Westman et al. - 2018 - Diglyme Based Electrolytes for Sodium-Ion Batterie.pdf;/Users/tn438/Zotero/storage/KDPW7FNN/acsaem.html},
  journal = {ACS Applied Energy Materials},
  number = {6}
}

